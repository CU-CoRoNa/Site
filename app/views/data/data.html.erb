<div class="boxed">
  <p> Our Data Sets | Descriptive Stats here </p>
</div>

<dl class="tabs" data-tab>
    <dd class="active"><a href="#browse">Browse</a></dd>
    <dd><a href="#search">Search</a></dd>
</dl>

<div class="tabs-content">
    <div class="content active" id="browse">
        <%= render partial: "data/entry" %>
    </div>
    <div class="content " id=search>
        <form action = <%= data_path %> method="get">
            <%= text_field_tag :q, params[:q] %>
            <%= submit_tag "Search", :name => nil %>
        </form>
        <%= render partial: "data/entry" %>
    </div>
</div>
